<?xml version='1.0' encoding='UTF-8'?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">
<managed-bean>
    <managed-bean-name>userController</managed-bean-name>
    <managed-bean-class>com.supinfo.supfriends.ejb.controller.UserController</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
</managed-bean>
<managed-bean>
    <managed-bean-name>editProfileController</managed-bean-name>
    <managed-bean-class>com.supinfo.supfriends.ejb.controller.editProfileController</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
</managed-bean>
<managed-bean>
    <managed-bean-name>addGroupController</managed-bean-name>
    <managed-bean-class>com.supinfo.supfriends.ejb.controller.addGroupController</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>

</managed-bean>
<managed-bean>
    <managed-bean-name>listGroupController</managed-bean-name>
    <managed-bean-class>com.supinfo.supfriends.ejb.controller.listGroupController</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
    <managed-bean-name>searchFriendController</managed-bean-name>
    <managed-bean-class>com.supinfo.supfriends.ejb.controller.searchFriendController</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
</managed-bean>
<managed-bean>
    <managed-bean-name>detailsGroupController</managed-bean-name>
    <managed-bean-class>com.supinfo.supfriends.ejb.controller.detailsGroupController</managed-bean-class>
    <managed-bean-scope>view</managed-bean-scope>
</managed-bean>

<managed-bean>
    <managed-bean-name>indexController</managed-bean-name>
    <managed-bean-class>com.supinfo.supfriends.ejb.controller.indexController</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
</managed-bean>
</faces-config>
